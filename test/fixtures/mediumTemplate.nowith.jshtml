<div>
	<h1 class='header'>@locals.header</h1>
	<h2 class='header2'>@locals.header2</h2>
	<h3 class='header3'>@locals.header3</h3>
	<h4 class='header4'>@locals.header4</h4>
	<h5 class='header5'>@locals.header5</h5>
	<h6 class='header6'>@locals.header6</h6>
	<ul class='list'>
	@for (var i = 0, l = locals.list.length; i < l; i++) {
		<li class='item'>@locals.list[i]</li>
	}
	</ul>
</div>